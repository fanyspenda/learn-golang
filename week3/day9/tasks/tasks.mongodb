// 6
db.books.aggregate(
    [
        {
            $lookup:{
                from: "authors",
                localField: "authorID",
                foreignField: "_id",
                as: "author"
            }
        },
        {
            $lookup:{
                from: "publishers",
                localField: "publisherID",
                foreignField: "_id",
                as: "publisher"
            }
        },
        {
            $unwind: {
                path: "$author",
            }
        },
        {
            $unwind: {
                path: "$publisher",
            }
        },
        {
            /**
             * _id: The id of the group.
             * fieldN: The first field name.
             */
            $group: {
              _id: "$_id",
              "_id": {
                  $concat: ["$author.firstName", " ", "$author.lastName"]
              },
              "number_of_books": {
                  $sum: 1
                },
                "list_of_books": {
                    $push: {
                        $concat: ["$title", " (", "$publisher.publisherName", ")"]
                    }
                }
            }
        }
    ]
)

// 7
db.books.aggregate([
    {
        $project: {
          _id: "$_id",
          "title": "$title",
          "price": "$price",
          "promo": {
              $cond: {
                  if: {
                      $lt: ["$price", 60000]
                  },
                  then: "1%",
                  else: {
                      $cond: {
                          if: {
                              $and: [
                                  {$gt: ["$price", 60000]}, 
                                  {$lt: ["$price", 90000]}
                                  ]
                            }, 
                            then: "2%",
                            else: "3%"
                        }
                    }
                }
            }
        }
    }
])

